{
  "name": "@aurelia-mdc-web/layout-grid",
  "version": "9.0.0-au2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Wrapper for Material Components Web Layout Grid",
  "keywords": [
    "aurelia",
    "material",
    "components",
    "web",
    "layout-grid"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-mdc-web/issues"
  },
  "license": "MIT",
  "author": "Maxim Balaganskiy <m.balaganskiy@gmail.com>",
  "contributors": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-mdc-web"
  },
  "files": [
    "dist",
    "*.scss"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:other": "copyfiles -u 1 src/**/*.html src/**/*.css dist",
    "build": "npm run build:ts && npm run build:other",
    "publish:au2": "npm publish --tag au2",
    "doc": "cross-env mkdirp doc && typedoc --json doc/api.json --excludeExternals --name aurelia-mdc-web-docs src/",
    "update-versions": "ts-node -P ../../tsconfig.scripts.json ../../scripts/update-versions.ts"
  },
  "dependencies": {
    "@aurelia-mdc-web/base": "9.0.0-au2",
    "@material/layout-grid": "^14.0.0"
  }
}
