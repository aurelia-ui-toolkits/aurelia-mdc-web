{
  "name": "@aurelia-mdc-web/all",
  "version": "8.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Wrappers for Material Components Web",
  "keywords": [
    "aurelia",
    "material",
    "components",
    "web"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-mdc-web/issues"
  },
  "license": "MIT",
  "author": "Maxim Balaganskiy <m.balaganskiy@gmail.com>",
  "contributors": [],
  "main": "dist/commonjs/index.js",
  "module": "dist/native-modules/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-mdc-web"
  },
  "files": [
    "dist",
    "*.scss"
  ],
  "scripts": {
    "build-ts:amd": "tsc -p tsconfig.build.json -t es5 -m amd      --outDir dist/amd",
    "build-ts:cjs": "tsc -p tsconfig.build.json -t es5 -m commonjs --outDir dist/commonjs",
    "build-ts:ntv": "tsc -p tsconfig.build.json -t es5 -m es6      --outDir dist/native-modules",
    "build-ts:es6": "tsc -p tsconfig.build.json -t es6 -m es6      --outDir dist/es2015",
    "build-ts:types": "tsc -p tsconfig.build.json --skipLibCheck -d --emitDeclarationOnly --declarationDir dist/types",
    "build-assets:amd": "copyfiles -u 1 src/**/*.html src/**/*.css dist/amd",
    "build-assets:cjs": "copyfiles -u 1 src/**/*.html src/**/*.css dist/commonjs",
    "build-assets:ntv": "copyfiles -u 1 src/**/*.html src/**/*.css dist/native-modules",
    "build-assets:es6": "copyfiles -u 1 src/**/*.html src/**/*.css dist/es2015",
    "prebuild": "rimraf dist",
    "build": "concurrently \"npm:build-ts:*\" && concurrently \"npm:build-assets:*\"",
    "doc": "cross-env mkdirp doc && typedoc --json doc/api.json --excludeExternals --name aurelia-mdc-web-docs src/",
    "update-versions": "ts-node -P ../../tsconfig.scripts.json ../../scripts/update-versions.ts",
    "publish:latest": "npm publish"
  },
  "dependencies": {
    "@aurelia-mdc-web/base": "8.0.2",
    "@aurelia-mdc-web/banner": "8.0.2",
    "@aurelia-mdc-web/button": "8.0.2",
    "@aurelia-mdc-web/card": "8.0.2",
    "@aurelia-mdc-web/checkbox": "8.0.2",
    "@aurelia-mdc-web/chips": "8.0.2",
    "@aurelia-mdc-web/circular-progress": "8.0.2",
    "@aurelia-mdc-web/data-table": "8.0.2",
    "@aurelia-mdc-web/dialog": "8.0.2",
    "@aurelia-mdc-web/drawer": "8.0.2",
    "@aurelia-mdc-web/elevation": "8.0.2",
    "@aurelia-mdc-web/expandable": "8.0.2",
    "@aurelia-mdc-web/fab": "8.0.2",
    "@aurelia-mdc-web/floating-label": "8.0.2",
    "@aurelia-mdc-web/form-field": "8.0.2",
    "@aurelia-mdc-web/icon": "8.0.2",
    "@aurelia-mdc-web/icon-button": "8.0.2",
    "@aurelia-mdc-web/image-list": "8.0.2",
    "@aurelia-mdc-web/layout-grid": "8.0.2",
    "@aurelia-mdc-web/line-ripple": "8.0.2",
    "@aurelia-mdc-web/linear-progress": "8.0.2",
    "@aurelia-mdc-web/list": "8.0.2",
    "@aurelia-mdc-web/lookup": "8.0.2",
    "@aurelia-mdc-web/menu": "8.0.2",
    "@aurelia-mdc-web/menu-surface": "8.0.2",
    "@aurelia-mdc-web/notched-outline": "8.0.2",
    "@aurelia-mdc-web/radio": "8.0.2",
    "@aurelia-mdc-web/ripple": "8.0.2",
    "@aurelia-mdc-web/segmented-button": "8.0.2",
    "@aurelia-mdc-web/select": "8.0.2",
    "@aurelia-mdc-web/slider": "8.0.2",
    "@aurelia-mdc-web/snackbar": "8.0.2",
    "@aurelia-mdc-web/switch": "8.0.2",
    "@aurelia-mdc-web/tab-bar": "8.0.2",
    "@aurelia-mdc-web/text-field": "8.0.2",
    "@aurelia-mdc-web/tooltip": "8.0.2",
    "@aurelia-mdc-web/top-app-bar": "8.0.2",
    "@aurelia-mdc-web/tree-view": "8.0.2",
    "@aurelia-mdc-web/typography": "8.0.2",
    "@aurelia-mdc-web/validation": "8.0.2"
  }
}