{
  "name": "aurelia-mdc-web",
  "private": true,
  "scripts": {
    "lint": "eslint -c .eslintrc.js packages/**/src/*.ts",
    "_test": "lerna run test",
    "_develop": "cross-env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" karma start --package=core",
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build",
    "start": "cd packages/app && npm start",
    "_predoc": "cross-env rimraf doc/api.json && rimraf dist/doc-temp && tsc --project tsconfig.json --outFile dist/doc-temp/aurelia-ux.js && node doc/shape-defs && copyfiles tsconfig.json dist/doc-temp",
    "doc": "lerna run doc",
    "_postdoc": "cross-env node doc/shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p angular -i docs/CHANGELOG.md -s",
    "_bump-version": "npm --no-git-tag-version version",
    "_preprepare-release": "cross-env npm run test",
    "_prepare-release": "cross-env npm run changelog && npm run build",
    "refresh": "lerna clean -y && npm run bootstrap",
    "deploy": "cd packages/app && npm run build && cd ../.. && deploy.sh",
    "ncu-upgrade": "lerna run ncu-upgrade"
  },
